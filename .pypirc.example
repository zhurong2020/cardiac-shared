# PyPI Configuration Template
# Copy this file to ~/.pypirc and fill in your token
# NEVER commit the actual .pypirc with real token!

[pypi]
username = __token__
password = pypi-YOUR_API_TOKEN_HERE

# Optional: TestPyPI for testing uploads
[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
password = pypi-YOUR_TEST_PYPI_TOKEN_HERE
